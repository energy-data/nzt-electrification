#!/bin/bash
#
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

PROJECT=$DIR/..

cd $PROJECT/dist/lib

while read -r line
do
    origin=$(echo -e $line | sed -e 's/\(.*\)[[:space:]]//g')
    destination=$(echo -e $line | sed -e 's/[[:space:]]\(.*\)//g')
    curl $origin -o $destination
done <$PROJECT/deps.dumb

cd -
